    $(document).ready(function () {
        var selectedPortfolio = "riskportfoliocss";
        $(".desktop-top-nav .top-nav-link").each(function (index) {
            if ($(this).data("css") !== undefined) {
                if ($(this).data("css") === selectedPortfolio) {
                    $(this).parent().addClass("selected");
                    $('.home-logo-link').attr("href", $(this).attr("href"));
                    $('.mobile-selected-link').attr("css", selectedPortfolio);
                    $('.mobile-selected-link').text($(this).text());
                }
            }
        });
    });
</script>

